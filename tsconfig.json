{
	"compilerOptions":{
		"strict":true,
		"module":"esnext",
		"target":"esnext",
		"moduleResolution":"bundler",
		"allowJs":true,
		"useDefineForClassFields":true,
		"importHelpers":true,
		"resolveJsonModule":true,
		"esModuleInterop":true,
		"baseUrl":".",
		"paths":{
			"@/*":[
				"src/*"
			]
		},
		"declaration":false
	},
	"include":[
		"src/**/*",
		"types/**/*"
	]
}
